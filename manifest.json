{
  "name": "Latency of Browser",
  "version": "0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google.com/jsapi https://www.google.com/uds/?file=visualization&v=1&packages=corechart ; object-src 'self'",

  "permissions":["http://*/*", "tabs", "storage","webNavigation"],

  "description": "ブラウザのレイテンシを表示する",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts":[
    {
        "matches": ["http://*/*"],
        "js":["script.js"]
    }
],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Show latency",
    "default_popup": "popup.html"
  }
}
